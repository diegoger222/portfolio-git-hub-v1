    "@nestjs/platform-express": "^10.0.2",
    "@nrwl/nx-plugin": "^19.8.4",
    "@nx/angular": "^20.6.0",
    "@nx/devkit": "20.5.0",
    "@prisma/client": "^6.5.0",
    "dotenv-flow": "^4.1.0",
    "dotenv-flow-cli": "^1.1.1",
    "nest-keycloak-connect": "^1.10.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
