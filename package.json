{
  "name": "@portfolio-git-hub-v1/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "generate": "pnpm nx g @portfolio-git-hub-v1/generator:ts-backend-module ",
    "generatetest": "pnpm nx g @portfolio-git-hub-v1/generator:ts-backend-module --dry-run --verbose  ",
    "dockerstart": "docker start w-backend",
    "run-backend": "pnpm nx serve w-n-backend",
    "run-frontend": "pnpm nx serve pd-book-frontend",
    "build": "pnpm nx build",
    "test": "pnpm nx test",
    "db:migrate-tm": "pnpm dotenv-flow -p apps/w-n-backend -- pnpm prisma migrate dev --schema=apps/w-n-backend/prisma/schema.prisma",
    "db:deploy-tm": "pnpm dotenv-flow -p apps/w-n-backend -- pnpm prisma migrate deploy --schema=apps/w-n-backend/prisma/schema.prisma",
    "db:generate-tm": "pnpm dotenv-flow -p apps/w-n-backend -- pnpm prisma generate --schema=apps/w-n-backend/prisma/schema.prisma",
    "__coment__": "This is forstart kafka server",
    "kafka:start": "docker-compose -f ./global/docker/docker-compose-kafka.yml up -d",
    "kafka:stop": "docker-compose -f ./global/docker/docker-compose-kafka.yml down",
    "w-backend:docker:start": "docker-compose -f ./apps/w-backend/docker-compose.yml up -d",
    "w-backend:docker:stop": "docker stop w-backend"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~19.2.0",
